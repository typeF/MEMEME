type User {
  id: ID! @unique
  username: String!
  email: String! @unique
  password: String!
  threads: [Thread!]!
  posts: [Post!]!
}

type SubForum {
  id: ID! @unique
  name: String!
  threads: [Thread!]!
}

type Thread {
  id: ID! @unique
  threadnumber: Int!
  author: User!
  title: String!
  posts: [Post!]!
}

type Post {
  id: ID! @unique
  postnumber: Int!
  author: User!
  content: String!
  thread: Thread!
}